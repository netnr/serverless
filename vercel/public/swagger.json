  "paths": {

    "/badge/nuget/v/{package}.svg": {
      "get": {
        "tags": [
          "Badge"
        ],
        "summary": "获取 NuGet 发布包的最新版本",
        "parameters": [
          {
            "name": "package",
            "description": "NuGet 发布包名",
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/badge/npm/v/{package}.svg": {
      "get": {
        "tags": [
          "Badge"
        ],
        "summary": "获取 NPM 发布包的最新版本",
        "parameters": [
          {
            "name": "package",
            "description": "NPM 发布包名",
