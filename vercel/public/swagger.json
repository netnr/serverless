    "/clock/{timezone}": {
      "get": {
        "tags": [
          "Common"
        ],
        "summary": "获取时钟（UTC），默认东8区，中国，自定义时区：东1~12区、西-1~-12区",
        "parameters": [
          {
            "name": "timezone",
            "description": "东1 ~ 12区、西-1 ~ -12区",
            "in": "path",
            "schema": {
              "type": "integer",
              "nullable": true
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/ip": {
      "get": {
        "tags": [
          "Common"
        ],
        "summary": "获取IP",
