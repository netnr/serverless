    "/DK/GetTable": {
      "get": {
        "tags": [
          "DK"
        ],
        "summary": "获取所有表名及注释",
        "parameters": [
          {
            "name": "tdb",
            "in": "query",
            "description": "数据库类型（0：MySQL，1：SQLite，2：Oracle，3：SQLServer，4：PostgreSQL）",
            "schema": {
              "$ref": "#/components/schemas/TypeDB"
            }
          },
          {
            "name": "conn",
            "in": "query",
            "description": "连接字符串",
            "schema": {
              "type": "string",
              "description": "连接字符串",
              "nullable": true
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ActionResultVM"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ActionResultVM"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ActionResultVM"
                }
              }
            }
          }
        }
      }
    },
    "/DK/GetColumn": {
      "post": {
        "tags": [
          "DK"
        ],
        "summary": "获取所有列",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "tdb": {
                    "$ref": "#/components/schemas/TypeDB"
                  },
                  "conn": {
                    "type": "string",
                    "description": "连接字符串",
                    "nullable": true
                  },
                  "filterTableName": {
                    "type": "string",
                    "description": "过滤表名，英文逗号分隔，为空时默认所有表",
                    "default": "",
                    "nullable": true
                  }
                }
              },
              "encoding": {
                "tdb": {
                  "style": "form"
                },
                "conn": {
                  "style": "form"
                },
                "filterTableName": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ActionResultVM"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ActionResultVM"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ActionResultVM"
                }
              }
            }
          }
        }
      }
    },
