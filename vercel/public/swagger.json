  "paths": {
    "/aip/ocr": {
      "post": {
        "tags": [
          "AI"
        ],
        "summary": "OCR 通用文字识别(百度接口,50000次/天免费,用自己申请的授权信息更稳定不受限制)",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "description": "待识别的图片文件（二选一）",
                    "format": "binary",
                    "nullable": true
                  },
                  "url": {
                    "type": "string",
                    "description": "远程图片地址（二选一）",
                    "nullable": true
                  },
                  "APP_ID": {
                    "type": "string",
                    "description": "百度AI接口：APP_ID（可选，用自己申请的授权信息更稳定不受限制）",
                    "nullable": true
                  },
                  "API_KEY": {
                    "type": "string",
                    "description": "百度AI接口：API_KEY（可选，用自己申请的授权信息更稳定不受限制）",
                    "nullable": true
                  },
                  "SECRET_KEY": {
                    "type": "string",
                    "description": "百度AI接口：SECRET_KEY（可选，用自己申请的授权信息更稳定不受限制）",
                    "nullable": true
                  }
                }
              },
              "encoding": {
                "file": {
                  "style": "form"
                },
                "url": {
                  "style": "form"
                },
                "APP_ID": {
                  "style": "form"
                },
                "API_KEY": {
                  "style": "form"
                },
                "SECRET_KEY": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/PublicResult"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PublicResult"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/PublicResult"
                }
              }
            }
          }
        }
      }
    },
    "/analysis": {
      "get": {
        "tags": [
          "AI"
        ],
        "summary": "内容分词解析(nodejieba 组件)",
        "parameters": [
          {
            "name": "lang",
            "in": "query",
            "description": "语言，默认 zh-cn",
            "schema": {
              "type": "string",
              "description": "语言，默认 zh-cn",
              "nullable": true
            }
          },
          {
            "name": "ctype",
            "in": "query",
            "description": "命令类型，0：分词（默认，可不传）；1：关键词",
            "schema": {
              "type": "integer",
              "description": "命令类型，0：分词（默认，可不传）；1：关键词",
              "format": "int32"
            }
          },
          {
            "name": "content",
            "in": "query",
            "description": "内容",
            "schema": {
              "type": "string",
              "description": "内容",
              "default": "结过婚的和尚未结过婚的",
              "nullable": true
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/PublicResult"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PublicResult"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/PublicResult"
                }
              }
            }
          }
        }
      }
    },
