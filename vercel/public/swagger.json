  "components": {
    "schemas": {
      "TypeDB": {
        "enum": [
          0,
          1,
          2,
          3,
          4,
          5
        ],
        "type": "integer",
        "format": "int32"
      },
      "ActionResultVM": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "msg": {
            "type": "string",
            "nullable": true
          },
          "data": {
            "type": "object",
            "additionalProperties": false,
            "nullable": true
          },
          "startTime": {
            "type": "string",
            "format": "date-time"
          },
          "useTime": {
            "type": "number",
            "format": "double",
            "readOnly": true
          },
          "endTime": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
